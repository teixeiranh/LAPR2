<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.ui.gui.ExportVaccinationStatisticsScene2">
    <bottom>
        <FlowPane alignment="CENTER" hgap="10.0" prefHeight="38.0" prefWidth="600.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="btnOK" mnemonicParsing="false" onAction="#btnOKAction" text="Invalid date! Please respect yyyy/MM/dd">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Button>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </FlowPane>
    </bottom>
    <center>
        <Label fx:id="lblNotification" BorderPane.alignment="CENTER">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
    </center>
</BorderPane>
